{
  "expo": {
    "name": "OLX",
    "scheme": "olx",
    "slug": "OLX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/OLX-v1b-small-RGB.png", // <-- app icon
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/OLX-v1b-small-RGB.png", // <-- splash logo
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/OLX-v1b-small-RGB.png", // <-- use your logo for adaptive icon
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.qwertyuiop246173.OLX"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c5cae861-38d8-499a-931d-c524942aa2e3"
      }
    },
    "owner": "olx001"
  }
}